{
    "cleanUrls": true,
    "rewrites": [
        {
            "source": "/remote/:token",
            "destination": "/rmt?token=:token"
        },
        {
            "source": "/remote/:token*",
            "destination": "/rmt?token=:token"
        }
    ],
    "redirects": [
        {
            "source": "/projects/better-websites/docs",
            "destination": "https://liamgen-jss.gitbook.io/better-websites"
        }
    ],
    "functions": {
        "api/proxy/*.php": {
            "runtime": "vercel-php@0.7.1"
        }
  }
}
